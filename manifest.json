{
    "manifest_version": 2,
    "name": "JwMethuselah",
    "description": "Jw Audio Auto Replay",
    "version": "1.0.0",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab"
    ],
    "default_language":"en",
    "content_scripts": [
      {
        "matches": [
          "https://www.jw.org/*"
        ],
        "js": [
          "jquery-3.4.1.min.js", "execute.js"
        ]
      }
    ]
  }