{
    "manifest_version": 2,
    "name": "JwMethuselah",
    "description": "Jw Auto Replay",
    "version": "1.0.0",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://www.jw.org/*"
        ],
        "js": [
          "execute.js"
        ]
      }
    ]
  }